   "source": [
    "from itertools import combinations_with_replacement\n",
    "\n",
    "string, k = input().split()\n",
    "k = int(k)\n",
    "\n",
    "combinations = list(combinations_with_replacement(sorted(string), k))\n",
    "\n",
    "for combination in combinations:\n",
    "    print(''.join(combination))\n"
   ]
